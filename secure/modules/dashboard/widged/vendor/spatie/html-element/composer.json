{
    "name": "spatie/html-element",
    "description": "Html rendering in php inspired by hyperscript",
    "keywords": [
        "spatie",
        "html-element"
    ],
    "homepage": "https://github.com/spatie/html-element",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\HtmlElement\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\HtmlElement\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
